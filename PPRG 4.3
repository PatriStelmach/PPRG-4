#include <iostream>
using namespace std;

int main() 
{
    int tab[10][10];
	
    for (int a = 0; a < 10; a++) 
	{
        for (int b = 0; b < 10; b++) 
		{
            if (a == 0) 
			{
                tab[b][0] = b;
            }
				
            else if (a == 1 ) 
			{
                tab[b][1] = b*2;
            }
				
            else if (a == 2) 
			{
                tab[b][2] = b*b;
            }
				
            else if (a == 3) 
			{
                tab[b][3] = b+4;
            }
				
            else if (a == 4) 
			{
                tab[b][4] = b-5;
            }
				
            else if (a >= 5) 
			{
                tab[b][a] = 0;
            }
        }
    }


	
    for (int a = 0; a < 10; a++) 
	{
        for (int b = 0; b < 10; b++) 
		{
            cout << tab[a][b] << "   ";
        }
        cout << "\n";
    } 
	
    return 0;
}
